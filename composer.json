{
    {
    "name": "notephp/notephp",
    "type": "project",
    "description": "The notephp Framework.",
    "keywords": [
        "framework",
        "notephp"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "Webapp/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "smarty/smarty" : "~3.1"
    }
}
